@model IEnumerable<Filtration.Models.LoggingContactsVisitor>

@{
    ViewBag.Title = "VisitorsList";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<section class="page-header">
    <h2>Список посетителей страницы КОНТАКТЫ</h2>
</section>

<section>
    @if (Model.Any())
    {
        <table class="table">
            <tr>
                <th>@Html.DisplayNameFor(model => model.SessionId)</th>
                <th>@Html.DisplayNameFor(model => model.UserLogin)</th>
                <th>@Html.DisplayNameFor(model => model.UserEmail)</th>
                <th>@Html.DisplayNameFor(model => model.VisitedDate)</th>
            </tr>

            @foreach (var item in Model) {
                <tr>
                    <td>@Html.DisplayFor(modelItem => item.SessionId)</td>
                    <td>@Html.DisplayFor(modelItem => item.UserLogin)</td>
                    <td>@Html.DisplayFor(modelItem => item.UserEmail)</td>
                    <td>@Html.DisplayFor(modelItem => item.VisitedDate)</td>
                </tr>
            }
        </table>
    }
    else
    {
        <p>В БД нет записей.</p>
    }
</section>


